            <RulesElement name="Cloud of Darkness" type="Power" internal-id="ID_FMP_POWER_2473"
                          url="http://www.wizards.com/dndinsider/compendium/power.aspx?id=2473" charelem="100"
                          legality="rules-legal">
                <specific name="Flavor">A shroud of blackness descends around you, hiding you from sight.</specific>
                <specific name="Power Usage">Encounter</specific>
                <specific name="Display">Drow Racial Power</specific>
